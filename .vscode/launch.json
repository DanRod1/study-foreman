{
    "name": "Python",
    "type": "python",
    "python":"${config.python.pythonPath}", 
    "request": "launch",
    "stopOnEntry": true,
    "console": "internalConsole",
    "program": "${file}",
    "cwd": "${workspaceRoot}",
    "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
    ],
    "env": {"name":"value"}
}
